<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="taqbir.alam.com.javaforfun">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="taqbir.alam.com.javaforfun.WelcomeActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="taqbir.alam.com.javaforfun.MainActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait"/>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity android:name="taqbir.alam.com.javaforfun.ActivityClass_Basics_PS"
            android:screenOrientation="portrait"/>
        <activity android:name="taqbir.alam.com.javaforfun.ActivityClass_Basics_JC"
            android:screenOrientation="portrait"/>
        <activity android:name="taqbir.alam.com.javaforfun.ActivityClass_Basics_VD"
            android:screenOrientation="portrait"></activity>
    </application>

</manifest>